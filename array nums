#include<iostream>
#include<string>

using namespace std;

int main()
{
    int n,i,j,flag=0;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<=n;i++)
    {
        for(j=0;j<n;j++)
        {
            if(i==a[j])
            {
                flag=1;

            }
        }
        if(flag==0)
        {
            cout<<i;
            return 0;
        }
        flag=0;
    }
    return 0;
}

